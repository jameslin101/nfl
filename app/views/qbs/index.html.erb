<h1>Listing qbs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Position</th>
    <th>Number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @qbs.each do |qb| %>
  <tr>
    <td><%= qb.name %></td>
    <td><%= qb.position %></td>
    <td><%= qb.number %></td>
    <td><%= link_to 'Show', qb %></td>
    <td><%= link_to 'Edit', edit_qb_path(qb) %></td>
    <td><%= link_to 'Destroy', qb, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Qb', new_qb_path %>
