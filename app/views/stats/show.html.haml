-if nfl_player.stats[0] then      
  %table.table.table-striped.table-bordered
    %thead
      %tr      
        -nfl_player.stats[0].stat_display_order.each do |label|
          %th
            =label
    %tbody
      -nfl_player.stats.each do |stat|
        %tr  
        -stat.stat_display_order.each do |label|
          %td
            =stat[label]
-else
  No Stats Available